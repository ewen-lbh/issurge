{
  "name": "issurge",
  "displayName": "issurge",
  "description": "Syntax highlighting for the issurge language",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.77.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "issurge",
        "aliases": [
          "Issurge issues list",
          "issurge"
        ],
        "extensions": [
          ".issurge",
          ".issues"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "issurge",
        "scopeName": "source.issurge",
        "path": "./syntaxes/issurge.tmLanguage.json",
        "embeddedLanguages": {
          "source.markdown": "markdown"
        }
      }
    ]
  }
}
